{
  "name": "vscode-twig",
  "displayName": "Twig",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {},
  "contributes": {
    "languages": [
      {
        "id": "twig",
        "aliases": [
          "HTML (Twig)",
          "twig"
        ],
        "extensions": [
          ".twig",
          ".html.twig"
        ],
        "configuration": "./languages/twig.configuration.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.2.6",
    "@types/vscode": "^1.79.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
